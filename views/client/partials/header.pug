header.header    
  nav.navbar.navbar-expand-lg.navbar-light.bg-light
    .container 
      a.navbar-brand(href='/topics')
        img.d-inline-block.align-top(
          src='data:image/jpeg;base64,' 
          width='30' 
          height='30' 
          alt='')
      a.navbar-brand(href='/topics') Music app
      button.navbar-toggler(
        type='button' 
        data-toggle='collapse' 
        data-target='#navbarSupportedContent' 
        aria-controls='navbarSupportedContent' 
        aria-expanded='false' 
        aria-label='Toggle navigation'
      )
        span.navbar-toggler-icon
        
      #navbarSupportedContent.collapse.navbar-collapse
        ul.navbar-nav.mr-auto
          li.nav-item
            a.nav-link(href="/topics") 
              <i class="fa-solid fa-house"></i> Trang chủ 
          li.nav-item
            a.nav-link(href="/topics") 
              <i class="fa-solid fa-music"></i> Chủ đề
          li.nav-item
            a.nav-link(href="/favorite-songs") 
              <i class="fa-solid fa-heart"></i> Yêu thích
          li.nav-item.dropdown
            a.nav-link.dropdown-toggle(href='#' role='button' data-toggle='dropdown' aria-expanded='false')
              <i class="fa-solid fa-user"></i> Tài khoản
            .dropdown-menu
              a.dropdown-item(href='/auth/login') Đăng nhập
              a.dropdown-item(href='/auth/register') Đăng kí
              .dropdown-divider
              a.dropdown-item(href='#') Đánh giá

        form.form-inline.my-2.my-lg-0.box-search(
            action="/search/result"
            method="GET"
          )
          input.form-control.mr-sm-2(
            type="text"
            placeholder="Nhập từ khóa..."
            class="form-control"
            name="keyword"
            value=keyword)
          button.btn.btn-outline-success.my-2.my-sm-0(type='submit') Tìm kiếm
        
          .inner-suggest
